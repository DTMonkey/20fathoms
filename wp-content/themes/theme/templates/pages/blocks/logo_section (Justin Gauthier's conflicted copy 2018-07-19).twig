<section class="section__logos">
  <div class="inner-container text-center">
    <h2>{{item.section_title}}</h2>
    <div class="logos">
      {% for logo in item.logos %}
        <div class="column desktop-col-1-3 tablet-col-1-2 mobile-col-1-1">
          <img src="{{logo.url}}" alt="{{logo.alt}}">
        </div>
      {% endfor %}
    </div>
  </div>
</section>